{
  "name": "sabertazimi.github.io",
  "type": "module",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "preview": "vite preview",
    "lint": "pnpm lint:style && pnpm lint:type-check",
    "lint:style": "eslint . && stylelint ./src/**/*.css",
    "lint:type-check": "tsc --noEmit",
    "lint:fix": "eslint . --fix && stylelint ./src/**/*.css --fix",
    "changeset": "commit-and-tag-version --dry-run -s",
    "release": "commit-and-tag-version -s"
  },
  "dependencies": {
    "react": "^19.2.0",
    "react-dom": "^19.2.0"
  },
  "devDependencies": {
    "@dg-scripts/eslint-config": "^5.21.7",
    "@dg-scripts/stylelint-config": "^5.21.7",
    "@types/node": "^24.10.1",
    "@types/react": "^19.2.5",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.1",
    "babel-plugin-react-compiler": "^1.0.0",
    "commit-and-tag-version": "^12.6.0",
    "eslint": "^9.39.1",
    "prettier": "^3.7.1",
    "prettier-plugin-tailwindcss": "^0.7.1",
    "stylelint": "^16.26.1",
    "typescript": "~5.9.3",
    "vite": "^7.2.4"
  }
}
